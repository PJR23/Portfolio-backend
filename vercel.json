{
    "version": 2,
    "builds": [
      {
        "src": "api/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/util/(.*)",
        "dest": "/api/util/$1"
      },
      {
        "src": "/api/files/(.*)",
        "dest": "/api/files/$1"
      },
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      }
    ]
  }
  